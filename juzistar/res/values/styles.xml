<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--utils.cropimg-->
    <style name="CropButton">
        <item name="android:layout_margin">1dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="wh_mw">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BottomPopStyle">
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/DialogAnimation</item>
    </style>
    <style name="DialogAnimation" parent="@android:style/Animation.Dialog">
        <item name="android:windowEnterAnimation">@anim/ssn_push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/ssn_push_bottom_out</item>
    </style>
    <style name="TopTtnLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="TopMenuIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/top_menu_img_max_size</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:minWidth">@dimen/top_menu_img_min_size</item>
        <item name="android:minHeight">@dimen/top_menu_img_min_size</item>
        <item name="android:maxHeight">@dimen/top_menu_img_max_size</item>
        <item name="android:maxWidth">@dimen/top_menu_img_max_size</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="top_menu_btn">
        <item name="android:textColor">@color/red_to_grey</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:clickable">false</item>
        <item name="android:focusable">false</item>
        <item name="android:textSize">@dimen/top_menu_btn_textsize</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="LoginPageEdt" parent="wh_mw">
        <item name="android:paddingLeft">@dimen/big_padding</item>
        <item name="android:paddingRight">@dimen/big_padding</item>
        <item name="android:textSize">@dimen/normal_font</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="EditRecAddressEdt" parent="wh_mw">
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:textSize">@dimen/normal_font</item>
        <item name="android:textColor">@color/normal_text</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/wbg_trans_stroke</item>
        <item name="android:layout_height">44dp</item>
    </style>

    <style name="NoTextCB">
        <item name="android:layout_width">15dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/blue_cb_bg</item>
    </style>

    <style name="LargeBtnWhiteText" parent="wh_mw">
        <item name="android:background">@drawable/login_btn_bg</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">@dimen/page_edg_mar</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="LargeWhiteBtnRedText" parent="wh_mw">
        <item name="android:background">@drawable/white_bg_grey_stroke_btn</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">@dimen/page_edg_mar</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">@color/red_text</item>
    </style>
    <style name="AppTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/AnimationSlide</item>
    </style>
    <style name="TabPageTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/TabPageAnimation</item>
    </style>
    <style name="TabPageAnimation" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/tab_page_alpha_in</item>
        <item name="android:activityOpenExitAnimation">@anim/ssn_stay_put</item>
        <item name="android:activityCloseEnterAnimation">@anim/ssn_stay_put</item>
        <item name="android:activityCloseExitAnimation">@anim/ssn_slide_right_out</item>
    </style>
    <style name="LoginPageTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/LoginPageAnimation</item>
    </style>
    <style name="LoginPageAnimation" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/ssn_push_bottom_in</item>
        <item name="android:activityOpenExitAnimation">@anim/ssn_stay_put</item>
        <item name="android:activityCloseEnterAnimation">@anim/ssn_stay_put</item>
        <item name="android:activityCloseExitAnimation">@anim/ssn_slide_right_out</item>
    </style>
    <style name="AnimationSlide" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/ssn_slide_right_in</item>
        <item name="android:activityOpenExitAnimation">@anim/ssn_stay_put</item>
        <item name="android:activityCloseEnterAnimation">@anim/ssn_stay_put</item>
        <item name="android:activityCloseExitAnimation">@anim/ssn_slide_right_out</item>
    </style>
    <style name="pop_dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@null</item>
        <item name="android:windowBackground">@drawable/dialog_insert</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="tab_rb_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">@dimen/normal_small_font</item>
        <item name="android:drawablePadding">0dp</item>
        <item name="android:textColor">@color/tab_selector_tv_color</item>
        <item name="android:gravity">center_horizontal|bottom</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="wh_ww">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="AddMinusNumBtn">
        <item name="android:layout_width">35dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black_press_white</item>
        <item name="android:background">@drawable/press_white_bg</item>
    </style>
    <style name="MyCommentRb">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">14sp</item>
        <item name="android:drawablePadding">@dimen/rb_tab_padding</item>
        <item name="android:textColor">@color/segment_tv_color</item>
        <item name="android:gravity">center_horizontal|bottom</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="UsualImgView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:scaleType">fitXY</item>
    </style>
    <style name="DisImgViewSmallVideoIcon">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:src">@drawable/icon_video</item>
    </style>
    <style name="Width0Weight1Line">
        <item name="android:background">@color/page_bg</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_width">0dp</item>
    </style>
    <style name="PreviewImg">
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/deep_alpha_black</item>
    </style>
    <style name="RedGreyRoundCb">
        <item name="android:layout_width">15dp</item>
        <item name="android:layout_height">15dp</item>
        <item name="android:button">@null</item>
        <item name="android:background">@drawable/red_grey_circle_sel</item>
        <item name="android:enabled">false</item>
    </style>
    <style name="GoodsImg">
        <item name="android:layout_width">@dimen/goods_img_size</item>
        <item name="android:layout_height">@dimen/goods_img_size</item>
        <item name="android:scaleType">centerCrop</item>
    </style>
    <style name="wStrokeWTextTBg">
        <item name="android:background">@drawable/tbg_white_stroke</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:paddingLeft">25dp</item>
        <item name="android:paddingRight">25dp</item>
        <item name="android:textSize">@dimen/normal_font</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="SplashAnimImg">
        <item name="android:layout_width">77dp</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    <style name="HLineWeight1Tv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@color/divider_line</item>
    </style>
    <style name="common_switch">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:switchMinWidth">70dp</item>
        <item name="android:textOn"></item>
        <item name="android:textOff"></item>
        <item name="android:thumb">@drawable/switch_selector_thumb</item>
        <item name="android:track">@drawable/switch_selector_bg</item>
    </style>
    <style name="ProductBotBtn">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/big_padding</item>
        <item name="android:paddingBottom">@dimen/big_padding</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/normal_font</item>
    </style>

    <style name="GlobalLV" parent="wh_mw">
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:divider">@null</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:listSelector">@android:color/transparent</item>
    </style>
    <style name="BlackAlphaRoundBtn">
        <item name="android:background">@drawable/black_alpha_corner_btn_bg</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">6dp</item>
        <item name="android:paddingBottom">6dp</item>
        <item name="android:paddingLeft">25dp</item>
        <item name="android:paddingRight">25dp</item>
        <item name="android:textSize">@dimen/normal_font</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <!-- Base application theme. -->
    <style name="Bubble.TextAppearance.Dark" parent="android:TextAppearance.Medium">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ff7f7f7f</item>
    </style>

    <style name="Bubble.TextAppearance.Light" parent="Bubble.TextAppearance.Dark">
        <item name="android:textColor">#ffeeeeee</item>
    </style>

    <style name="ClusterIcon.TextAppearance" parent="android:TextAppearance">
        <item name="android:textColor">#ffeeeeee</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>